{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "extends": [
    "config:recommended"
  ],
  "poetry": {
    "enabled": true
  },
  "pep621": {
    "enabled": false
  },
  "npm": {
    "enabled": true
  },
  "packageRules": [
    {
      "description": "[All] Ignore all packages initially",
      "enabled": false,
      "matchPackageNames": [
        "*"
      ],
      "labels": ["dependencies"]
    }
  ]
}